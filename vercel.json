{
    "version": 2,
    "builds": [
      {
        "src": "Backend_MediStore/MediStore/manage.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "staticfiles/**/*", 
        "use": "@vercel/static",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Backend_MediStore/MediStore/manage.py"
      }
    ]
  }